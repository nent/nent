{
  "headers": [
    {
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=7200"
      }]
    },
    {
      "source" : "**/*.@(js}|css)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=500"
      }]
    }
  ],
  "rewrites": [
    {
      "source": "/home/**",
      "destination": "/index.html"
    },
    {
      "source": "/start/**",
      "destination": "/index.html"
    },
    {
      "source": "/app/**",
      "destination": "/index.html"
    },
    {
      "source": "/actions/**",
      "destination": "/index.html"
    },
    {
      "source": "/routing/**",
      "destination": "/index.html"
    },
    {
      "source": "/content/**",
      "destination": "/index.html"
    },
    {
      "source": "/data/**",
      "destination": "/index.html"
    },
    {
      "source": "/audio/**",
      "destination": "/index.html"
    },
    {
      "source": "/presentations/**",
      "destination": "/index.html"
    },
    {
      "source": "/elements/**",
      "destination": "/index.html"
    },
    {
      "source": "/components/**",
      "destination": "/index.html"
    }
  ]
}
