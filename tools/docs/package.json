{
  "name": "@nent/docs",
  "private": false,
  "version": "0.8.3",
  "description": "nent docs",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "collection": "./dist/collection/collection-manifest.json",
  "types": "./dist/types/components.d.ts",
  "scripts": {
    "--- development ---": "---------------",
    "start": "run-p watch serve",
    "serve": "stencil build --dev --watch --serve",
    "new": "stencil generate",
    "watch": "npm-watch",
    "copy": "cd src && copyfiles ./pages/**/*.*  ./examples/**/*.* ./assets/**/*.* ../../../docs/",
    "--- production ---": "---------------",
    "clean": "rm -fR ../../docs",
    "build": "stencil build --www",
    "build:ssg": "stencil build --prerender",
    "build:dev": "stencil build --dev --www",
    "postbuild": "npm run copy",
    "publish": ""
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@stencil/core": "2.17.0",
    "@stencil/postcss": "2.1.0",
    "@stencil/sass": "1.5.2",
    "@types/tailwindcss": "^3.0.3",
    "autoprefixer": "10.4.7",
    "bootstrap": "5.1.3",
    "postcss": "8.4.14",
    "postcss-import": "14.1.0",
    "postcss-scss": "4.0.4",
    "remixicon": "2.5.0",
    "tailwindcss": "3.1.4",
    "workbox-build": "6.5.3",
    "workbox-window": "6.5.3"
  },
  "license": "MIT",
  "watch": {
    "build:dev": {
      "patterns": [
        "src/**/*.*"
      ]
    }
  },
  "dependencies": {
    "@types/tailwindcss": "^3.0.3"
  }
}
