{
  "name": "@nent/docs",
  "private": false,
  "version": "0.8.3",
  "description": "nent docs",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "collection": "./dist/collection/collection-manifest.json",
  "types": "./dist/types/components.d.ts",
  "scripts": {
    "--- development ---": "---------------",
    "start": "run-p watch serve",
    "serve": "stencil build --dev --watch --serve",
    "new": "stencil generate",
    "watch": "npm-watch",
    "copy": "cd src && copyfiles ./pages/**/*.*  ./examples/**/*.* ./assets/**/*.* ../../../docs/",
    "--- production ---": "---------------",
    "clean": "rm -fR ../../docs",
    "build": "stencil build --www",
    "build:ssg": "stencil build --prerender",
    "build:dev": "stencil build --dev --www",
    "postbuild": "npm run copy",
    "publish": ""
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "6.0.0",
    "@stencil/core": "4.21.0",
    "@stencil/postcss": "2.1.0",
    "@stencil/sass": "3.0.12",
    "@types/tailwindcss": "^3.1.0",
    "autoprefixer": "10.4.20",
    "bootstrap": "5.1.3",
    "postcss": "8.4.44",
    "postcss-import": "16.1.0",
    "postcss-scss": "4.0.9",
    "remixicon": "4.3.0",
    "tailwindcss": "3.4.10",
    "workbox-build": "7.1.1",
    "workbox-window": "7.1.0"
  },
  "license": "MIT",
  "watch": {
    "build:dev": {
      "patterns": [
        "src/**/*.*"
      ]
    }
  },
  "dependencies": {
  }
}
