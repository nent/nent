{
  "name": "@nent/cli",
  "version": "0.0.0",
  "description": "The utility CLI for managing NEET sites easily.",
  "author": "Jason Wicker",
  "bin": {
    "nent": "./bin/run"
  },
  "homepage": "https://github.com/nent/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "nent/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "htmlparser2": "^8.0.1",
    "xml-sitemap": "^0.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/node": "^16.9.4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "nent",
    "dirname": "nent",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "sitemap": {
        "description": "Generate a sitemap from a single HTML file and its NENT router."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "jest  \"**/*.spec.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/nent/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
