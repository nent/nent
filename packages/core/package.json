{
  "name": "@nent/core",
  "version": "0.4.1",
  "description": "nent core components",
  "author": "support <support@nent.dev> (https://nent.dev)",
  "homepage": "https://nent.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/nent/nent",
    "directory": "packages/core"
  },
  "private": false,
  "sideEffects": false,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nent/nent/issues",
    "email": "tech@nent.dev"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/custom-elements/index.js",
  "es2015": "./dist/esm/index.js",
  "es2017": "./dist/esm/index.js",
  "types": "./dist/types/components.d.ts",
  "loader": "./dist/loader",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/nent/nent.esm.js",
  "custom-elements": "./dist/custom-elements.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "--- development ---": "---------------",
    "start": "run-p watch:ts watch",
    "watch:ts": "stencil build -c stencil.config.docs.ts --watch",
    "new": "stencil generate",
    "copy": "copyfiles -u ./src/components/**/*.md ../../docs/pages/components",
    "watch": "npm-watch",
    "--- production ---": "---------------",
    "build": "stencil build --docs-readme",
    "build:docs": "stencil build -c stencil.config.docs.ts",
    "--- test ---": "---------------",
    "test": "stencil test --spec --coverage",
    "test:watch": "stencil test --dev --spec --watch"
  },
  "devDependencies": {
    "@stencil/core": "2.5.2",
    "@stencil/sass": "1.4.1",
    "@stencil/store": "1.4.1",
    "@types/howler": "2.2.2",
    "eslint-plugin-clean-regex": "0.5.1",
    "expr-eval": "2.0.2",
    "jsonata": "1.8.4",
    "remarkable": "2.0.1"
  },
  "jest": {
    "preset": "@stencil/core/testing",
    "testPathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "watch": {
    "copy": {
      "test": "src/**/*.md"
    }
  },
  "gitHead": "229559fdf5d8b2539a6288c0e96092f59d5e3a25"
}
